class Human:
    def __init__(self, name, age, gender):
        self.name = name
        self.age = age
        self.gender = gender

    def check_adult(self):
        if self.age >= 20:
            return "大人です"
        else:
            return "大人ではありません"

# インスタンスを作成
Human_data = Human("侍太郎", 36, "男性")

# 年齢を確認し、その結果を出力
result = Human_data.check_adult()
print(result)
